<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@drawable/noisebg"
    >
    <RelativeLayout  
    	android:id="@+id/astitle"
	    android:orientation="horizontal"
	    android:layout_width="fill_parent"
	    android:layout_height="42dp"
	    android:layout_alignParentTop="true"
	    android:background="@drawable/title_gradient">
	    <RelativeLayout
	    	android:layout_width="wrap_content"
	    	android:layout_height="fill_parent">
	    	<!-- 
	    	<ImageView
				android:id="@+id/titleicon"
				android:layout_width="wrap_content"
				android:layout_height="40px"
				android:paddingLeft="25px"
				android:src="@drawable/titleicon"/>
			-->
		    <TextView android:layout_width="wrap_content"
		        android:layout_height="42dp" android:id="@+id/title"
		        android:text="[android swim]" 
		        android:textColor="#eaeaea"
		        android:paddingLeft="13px"
		        android:paddingBottom="8dp"
		        android:gravity="bottom"
		        android:textSize="16dp"
		        android:textStyle="normal"
		        android:shadowColor="#aa000000"
				android:shadowDx="-3.0"
				android:shadowDy="2.0"
				android:shadowRadius="1.0"/>
			
		</RelativeLayout>
	    <LinearLayout
	    	android:layout_width="fill_parent"
	    	android:layout_height="fill_parent"
	    	android:orientation="horizontal"
	    	android:gravity="right">
		
			<ImageView
				android:id="@+id/vdiv"
				android:layout_width="5dp"
				android:layout_height="42dp"
				android:paddingRight="3dp"
				android:paddingTop="5dp"
				android:src="@drawable/vdivider"
				/>
				
			<ImageButton
				android:id="@+id/lst"
				android:layout_width="42dp"
				android:layout_height="42dp"
				android:paddingRight="5dp"
				android:paddingLeft="2dp"
				android:gravity="center"
				android:src="@drawable/homepixel"
				android:background="@null"
				/>
				
			<ImageView
				android:id="@+id/vdiv"
				android:layout_width="5dp"
				android:layout_height="42dp"
				android:paddingRight="3dp"
				android:paddingTop="5dp"
				android:src="@drawable/vdivider"
				/>
			<ImageButton
				android:id="@+id/rep"
				android:layout_width="42dp"
				android:layout_height="42dp"
				android:paddingRight="3dp"
				android:paddingLeft="2dp"
				android:gravity="center"
				android:src="@drawable/messagepixel"
				android:background="@null"
				/>
		</LinearLayout>
    </RelativeLayout>
    <ScrollView
    	android:layout_width="fill_parent"
    	android:layout_height="fill_parent"
    	android:layout_below="@+id/astitle">
    <TableLayout
    	android:id="@+id/starticons"
		android:layout_width="fill_parent"
		android:layout_height="wrap_content"
		android:layout_below="@+id/astitle"
		android:stretchColumns="0,1"
		android:paddingTop="12dp">
		<TableRow>
	    	<RelativeLayout
	    		android:layout_width="fill_parent"
                android:layout_height="wrap_content">
	    		<TextView 
	    			android:textSize="14dp" 
	    			android:id="@+id/textView1" 
	    			android:text="boards" 
	    			android:gravity="bottom|center_horizontal" 
	    			android:layout_height="wrap_content"
	    			android:layout_width="fill_parent"   
	    			android:textColor="#363636" 
	    			android:layout_below="@+id/board_icon"
	    			android:paddingTop="5dp"
	    			></TextView>
	    		<ImageButton 
	    			android:background="@null" 
	    			android:id="@+id/board_icon" 
	    			android:layout_height="wrap_content" 
	    			android:layout_width="wrap_content" 
	    			android:src="@drawable/boards" 
	    			android:layout_centerInParent="true"></ImageButton>
			</RelativeLayout>
			<RelativeLayout
	    		android:layout_width="fill_parent"
                android:layout_height="wrap_content">
	    		<TextView 
	    			android:textSize="14dp" 
	    			android:id="@+id/textView1" 
	    			android:text="subscriptions" 
	    			android:gravity="bottom|center_horizontal" 
	    			android:layout_height="wrap_content"
	    			android:layout_width="fill_parent"  
	    			android:textColor="#363636" 
	    			android:layout_below="@+id/subs_icon"
	    			android:paddingTop="5dp"
	    			></TextView>
	    		<ImageButton 
	    			android:background="@null" 
	    			android:id="@+id/subs_icon" 
	    			android:layout_height="wrap_content" 
	    			android:layout_width="wrap_content" 
	    			android:src="@drawable/subscriptions" 
	    			android:layout_centerInParent="true"></ImageButton>
			</RelativeLayout>
		</TableRow>
		<TableRow android:paddingTop="22dp">
	    	<RelativeLayout
	    		android:layout_width="fill_parent"
                android:layout_height="wrap_content">
	    		<TextView 
	    			android:textSize="14dp" 
	    			android:id="@+id/textView1" 
	    			android:text="tracker" 
	    			android:gravity="bottom|center_horizontal" 
	    			android:layout_height="wrap_content"
	    			android:layout_width="fill_parent"  
	    			android:textColor="#363636" 
	    			android:layout_below="@+id/tracker_icon"
	    			android:paddingTop="5dp"
	    			></TextView>
	    		<ImageButton 
	    			android:background="@null" 
	    			android:id="@+id/tracker_icon" 
	    			android:layout_height="wrap_content" 
	    			android:layout_width="wrap_content" 
	    			android:src="@drawable/tracker" 
	    			android:layout_centerInParent="true"></ImageButton>
			</RelativeLayout>
			<RelativeLayout
	    		android:layout_width="fill_parent"
                android:layout_height="wrap_content">
	    		<TextView 
	    			android:textSize="14dp" 
	    			android:id="@+id/textView1" 
	    			android:text="inbox" 
	    			android:gravity="bottom|center_horizontal" 
	    			android:layout_height="wrap_content"
	    			android:layout_width="fill_parent"  
	    			android:textColor="#363636" 
	    			android:layout_below="@+id/message_icon"
	    			android:paddingTop="5dp"
	    			></TextView>
	    		<ImageButton 
	    			android:background="@null" 
	    			android:id="@+id/message_icon" 
	    			android:layout_height="wrap_content" 
	    			android:layout_width="wrap_content" 
	    			android:src="@drawable/messages" 
	    			android:layout_centerInParent="true"></ImageButton>
			</RelativeLayout>
		</TableRow>
		<TableRow android:paddingTop="22dp">
	    	<RelativeLayout
	    		android:layout_width="fill_parent"
                android:layout_height="wrap_content">
	    		<TextView 
	    			android:textSize="14dp" 
	    			android:id="@+id/textView1" 
	    			android:text="profile" 
	    			android:gravity="bottom|center_horizontal" 
	    			android:layout_height="wrap_content"
	    			android:layout_width="fill_parent"  
	    			android:textColor="#363636" 
	    			android:layout_below="@+id/profile_icon"
	    			android:paddingTop="5dp"
	    			></TextView>
	    		<ImageButton 
	    			android:background="@null" 
	    			android:id="@+id/profile_icon" 
	    			android:layout_height="wrap_content" 
	    			android:layout_width="wrap_content" 
	    			android:src="@drawable/log_in" 
	    			android:layout_centerInParent="true"></ImageButton>
			</RelativeLayout>
			<RelativeLayout
	    		android:layout_width="fill_parent"
                android:layout_height="wrap_content">
	    		<TextView 
	    			android:textSize="14dp" 
	    			android:id="@+id/textView1" 
	    			android:text="settings" 
	    			android:gravity="bottom|center_horizontal" 
	    			android:layout_height="wrap_content"
	    			android:layout_width="fill_parent"  
	    			android:textColor="#363636" 
	    			android:layout_below="@+id/settings_icon"
	    			android:paddingTop="5dp"
	    			></TextView>
	    		<ImageButton 
	    			android:background="@null" 
	    			android:id="@+id/settings_icon" 
	    			android:layout_height="wrap_content" 
	    			android:layout_width="wrap_content" 
	    			android:src="@drawable/preferences" 
	    			android:layout_centerInParent="true"></ImageButton>
			</RelativeLayout>
		</TableRow>
		<TableRow android:paddingTop="22dp">
	    	<RelativeLayout
	    		android:layout_width="fill_parent"
                android:layout_height="wrap_content">
	    		<TextView 
	    			android:textSize="14dp" 
	    			android:id="@+id/textView1" 
	    			android:text="search" 
	    			android:gravity="bottom|center_horizontal" 
	    			android:layout_height="wrap_content"
	    			android:layout_width="fill_parent"  
	    			android:textColor="#363636" 
	    			android:layout_below="@+id/search_icon"
	    			android:paddingTop="5dp"
	    			></TextView>
	    		<ImageButton 
	    			android:background="@null" 
	    			android:id="@+id/search_icon" 
	    			android:layout_height="wrap_content" 
	    			android:layout_width="wrap_content" 
	    			android:src="@drawable/search" 
	    			android:layout_centerInParent="true"></ImageButton>
			</RelativeLayout>
			
		</TableRow>
    </TableLayout>
    </ScrollView>
</RelativeLayout>